        "type": "stable"
    },
    "pyopenssl": {
        "command": "pip install pyopenssl",
        "type": "stable"
    },
    "pyIsEmail": {
        "command": "pip install pyIsEmail",
        "type": "stable"
    },
    "psutil": {
        "command": "pip install psutil",
        "type": "stable"
    },
    "websocket": {
        "command": "pip install websocket",
        "type": "stable"
    },
    "websocket-client": {
        "command": "pip install websocket-client",
